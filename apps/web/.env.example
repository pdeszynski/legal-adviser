# Backend API Configuration
# The GraphQL endpoint URL - must match the backend server port
# Backend runs on port 3001 to avoid conflict with Next.js on port 3000
NEXT_PUBLIC_GRAPHQL_URL=http://localhost:3001/graphql
# The REST API endpoint URL (for document streaming, etc.)
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# AI Engine Configuration
# The AI Engine URL for streaming chat responses
NEXT_PUBLIC_AI_ENGINE_URL=http://localhost:8000

# Analytics Configuration
# Google Analytics 4 Measurement ID (optional)
# Format: G-XXXXXXXXXX
# Leave empty to disable analytics tracking
NEXT_PUBLIC_GA_MEASUREMENT_ID=
